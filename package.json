{
  "name": "health-fitness-tracker",
  "version": "1.0.0",
  "description": "Online Health & Fitness Tracker MVP - MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
